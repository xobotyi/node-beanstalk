{
  "name": "node-beanstalk",
  "version": "1.5.12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xobotyi/node-beanstalk.git"
  },
  "bugs": {
    "url": "https://github.com/react-hookz/web/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.41",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.1.1",
    "@types/jest": "^26.0.22",
    "@xobotyi/eslint-config": "^2.2.0",
    "commitlint": "^12.1.1",
    "eslint": "^7.24.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typedoc-neo-theme": "^1.1.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "docs:buid": "yarn typedoc",
    "build": "yarn build:cleanup && yarn build:tsc",
    "build:cleanup": "rimraf ./dist",
    "build:tsc": "tsc -p ./tsconfig.build.json",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint . --ext ts,js,tsx,jsx",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "prepublishOnly": "yarn lint && yarn build && yarn test"
  },
  "lint-staged": {
    "*.[tj]s?x": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
